{
	"name": "@kitql/kit-feedback",
	"version": "0.0.5",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jycouet/kitql",
		"directory": "packages/modules/kit-modules/kitFeedback"
	},
	"scripts": {
		"gen:style": "npx tailwindcss -c ./tailwind.config.cjs -o ./dist/app.postcss --minify",
		"package": "rm -rf ./dist && mkdir ./dist && cp ./src/* ./dist -r && cp ./package.json ./dist/package.json"
	},
	"devDependencies": {},
	"dependencies": {
		"@felte/validator-vest": "^1.0.9",
		"felte": "^1.2.2",
		"reflect-metadata": "^0.1.13",
		"vest": "^4.4.0"
	},
	"peerDependencies": {},
	"type": "module",
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./index.ts",
			"import": "./index.ts"
		},
		"./*": {
			"require": "./*.ts",
			"import": "./*.ts"
		}
	}
