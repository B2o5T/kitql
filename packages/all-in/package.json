{
	"name": "@kitql/all-in",
	"version": "0.5.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jycouet/kitql",
		"directory": "packages/all-in"
	},
	"scripts": {
		"prepack": "bob prepack"
	},
	"devDependencies": {},
	"dependencies": {
		"@graphql-codegen/cli": "2.6.2",
		"@graphql-codegen/graphql-modules-preset": "2.3.8",
		"@graphql-codegen/introspection": "2.1.1",
		"@graphql-codegen/typed-document-node": "2.2.8",
		"@graphql-codegen/typescript": "2.4.8",
		"@graphql-codegen/typescript-document-nodes": "2.2.8",
		"@graphql-codegen/typescript-operations": "2.3.5",
		"@graphql-codegen/typescript-resolvers": "2.6.1",
		"@graphql-eslint/eslint-plugin": "3.9.1",
		"@graphql-typed-document-node/core": "3.1.1",
		"@kitql/comp": "0.1.1",
		"@kitql/client": "0.4.1",
		"@kitql/graphql-codegen": "0.4.0",
		"@kitql/module-codegen": "0.2.0",
		"@kitql/vite-plugin-watch-and-run": "0.2.0",
		"graphql": "16.3.0"
	},
	"peerDependencies": {},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.js",
			"import": "./dist/*.mjs"
		}
	},
	"typings": "dist/index.d.ts",
	"typescript": {
		"definition": "dist/index.d.ts"
	},
	"buildOptions": {
		"input": "./src/index.ts"
	},
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	}
}
