{
	"name": "@kitql/all-in",
	"version": "0.0.28",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jycouet/kitql.git",
		"directory": "packages/all-in"
	},
	"scripts": {
		"clean": "rm -rf ./dist",
		"test": "jest --no-watchman --config ../../jest.config.js",
		"prepack": "bob prepack"
	},
	"devDependencies": {},
	"dependencies": {
		"@graphql-codegen/cli": "2.6.2",
		"@graphql-codegen/graphql-modules-preset": "2.3.4",
		"@graphql-codegen/typed-document-node": "2.2.4",
		"@graphql-codegen/typescript": "2.4.4",
		"@graphql-codegen/typescript-document-nodes": "2.2.4",
		"@graphql-codegen/typescript-operations": "2.3.1",
		"@graphql-codegen/typescript-resolvers": "2.5.1",
		"@graphql-typed-document-node/core": "3.1.1",
		"@kitql/client": "0.0.29",
		"@kitql/graphql-codegen": "0.0.16",
		"@kitql/vite-plugin-watch-and-run": "0.0.6"
	},
	"peerDependencies": {
		"graphql": "^15.0.0 || ^16.0.0"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.js",
			"import": "./dist/*.mjs"
		}
	},
	"typings": "dist/index.d.ts",
	"typescript": {
		"definition": "dist/index.d.ts"
	},
	"buildOptions": {
		"input": "./src/index.ts"
	},
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	}
}
