{
	"name": "@kitql/client",
	"version": "0.0.28",
	"repository": {
		"type": "git",
		"url": "https://github.com/jycouet/kitql.git",
		"directory": "packages/client"
	},
	"license": "MIT",
	"scripts": {
		"lint": "eslint **/*.ts",
		"test": "jest --no-watchman --config ../../jest.config.js",
		"prepack": "bob prepack"
	},
	"dependencies": {
		"@kitql/helper": "0.0.4",
		"graphql": "16.3.0"
	},
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.js",
			"import": "./dist/*.mjs"
		}
	},
	"typings": "dist/index.d.ts",
	"typescript": {
		"definition": "dist/index.d.ts"
	},
	"buildOptions": {
		"input": "./src/index.ts"
	},
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"engines": {
		"node": "17.4.0"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	}
}
