{
	"name": "demo1",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"prepare": "yarn gen",
		"dev": "svelte-kit dev --port 3777",
		"gen": "graphql-codegen --config ./.graphqlrc.yaml",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"quality": "yarn quality:eslint && yarn quality:prettier && yarn quality:ts",
		"quality:fix": "prettier --ignore-path .gitignore --write --plugin-search-dir=. . && yarn quality:eslint --fix",
		"quality:eslint": "eslint --ignore-path .gitignore .",
		"quality:prettier": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"quality:ts": "svelte-check --tsconfig ./tsconfig.json",
		"quality:ts:w": "yarn quality:ts --watch",
		"update": "npm exec -- npx ncu -u -p yarn"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "5.12.0",
		"@typescript-eslint/parser": "5.12.0",
		"eslint": "8.9.0",
		"eslint-config-prettier": "8.4.0",
		"eslint-plugin-svelte3": "3.4.0",
		"prettier": "2.5.1",
		"prettier-plugin-svelte": "2.6.0",
		"svelte": "3.46.4",
		"svelte-check": "2.4.5",
		"svelte-preprocess": "4.10.3",
		"tslib": "2.3.1",
		"typescript": "4.5.5"
	},
	"dependencies": {
		"@kitql/all-in": "0.1.2",
		"graphql": "16.3.0"
	},
	"type": "module",
	"engines": {
		"node": ">=16.0.0"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	}
}
